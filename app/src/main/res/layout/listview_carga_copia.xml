<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">


    <TextView
        android:id="@+id/textView_3a"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="2dp"
        android:maxLines="1"
        android:text=""
        android:textAppearance="@android:style/TextAppearance.Material"
        android:textSize="12sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/textView_list_carga_lote"
        app:layout_constraintTop_toBottomOf="@+id/textView_of_leg"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1"
        app:layout_constraintHorizontal_bias="0.0" />

    <TextView
        android:id="@+id/textView_list_carga_lote"
        android:layout_width="140dp"
        android:layout_height="wrap_content"
        android:layout_marginRight="8dp"
        android:layout_marginTop="2dp"
        android:maxLines="1"
        android:text=""
        android:textAppearance="@android:style/TextAppearance.Material"
        android:textSize="12sp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_of_leg"
        tools:layout_conversion_absoluteHeight="14dp"
        tools:layout_conversion_absoluteWidth="140dp"
        tools:layout_conversion_absoluteX="0dp"
        tools:layout_conversion_absoluteY="15dp"
        android:layout_marginEnd="8dp" />

    <TextView
        android:id="@+id/textView_list_carga_leg_localizacao"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="2dp"
        android:text="Localização: "
        android:textAppearance="@android:style/TextAppearance.Material"
        android:textStyle="bold"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_3a" />

    <TextView
        android:id="@+id/textView_list_carga_qt"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="5dp"
        android:layout_marginTop="2dp"
        android:maxLines="1"
        android:text=""
        android:textAppearance="@android:style/TextAppearance.Material"
        app:layout_constraintLeft_toRightOf="@+id/textView_list_carga_leg_qt"
        app:layout_constraintTop_toBottomOf="@+id/textView_list_carga_leg_localizacao"
        tools:layout_conversion_absoluteHeight="14dp"
        tools:layout_conversion_absoluteWidth="0dp"
        tools:layout_conversion_absoluteX="360dp"
        tools:layout_conversion_absoluteY="8dp"
        android:layout_marginStart="5dp" />

    <TextView
        android:id="@+id/textView_of_leg"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="2dp"
        android:maxLines="1"
        android:textAppearance="@android:style/TextAppearance.Material"
        app:layout_constraintHorizontal_bias="0.0"

        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView_list_carga_leg_qt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="2dp"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="2dp"
        android:maxLines="1"
        android:text="Qt:"
        android:textAppearance="@android:style/TextAppearance.Material"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_list_carga_leg_localizacao"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView_list_carga_qt_alt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="2dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="64dp"
        android:layout_marginStart="8dp"
        android:maxLines="1"
        android:textAppearance="@android:style/TextAppearance.Material"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="2dp"
        app:layout_constraintTop_toBottomOf="@+id/textView_list_carga_leg_localizacao"
        app:layout_constraintVertical_bias="0.0"
        android:layout_marginLeft="32dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintHorizontal_bias="0.562" />

    <TextView
        android:id="@+id/textView_list_carga_localizacao"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="2dp"
        android:maxLines="1"
        android:textAppearance="@android:style/TextAppearance.Material"
        app:layout_constraintLeft_toRightOf="@+id/textView_list_carga_leg_localizacao"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_3a"
        app:layout_constraintHorizontal_bias="0.0" />


</android.support.constraint.ConstraintLayout>